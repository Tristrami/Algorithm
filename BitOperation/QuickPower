#include <iostream>

using namespace std;
typedef long long ll;

ll quick_pow(ll a, ll b, ll p)
{
    ll ans = 1 % p;
    while (b)
    {
        if (b & 1) ans = ans * a % p;
        b >>= 1;
        a = a * a % p;
    }
    return ans;
}

int main()
{
    ll a, b, p;
    cin >> a >> b >> p;
    cout << quick_pow(a, b, p);
    return 0;
}
